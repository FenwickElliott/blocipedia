<div class='container'>
  <div class="row">
    <div class="col-md-8">
      <h1> <%= @wiki.title %> </h1>
      <p> <%= @wiki.body %> </p>
    </div>
      <% if current_user %>
      <div class='col-md-3 col-md-offset-1'>
        <%= link_to 'Edit', edit_wiki_path, class: 'btn btn-success' %>
        <%= link_to 'Delete', @wiki, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure?' } %>
      </div>
    <% end %>
  </div>
</div>
