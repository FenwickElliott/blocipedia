<div class='container'>
  <div class='row'>
    <div class='col-md-offset-4'>
      <div class='col-md-4'>
        <%= form_for @wiki do |f| %>
          <%= f.label :title %>
          <%= f.text_field :title %>

          <%= f.label :body %>
          <%= f.text_area :body %>

          <%= f.label :private %>
          <%= f.check_box :private %>

          <%= f.submit "Save" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
